 }\n  },\n}\n</script>\n\n<style lang=\"scss\">\n.login {\n  display: block;\n  width: 50rem;\n  padding: 2.4rem;\n  border-radius: 3.2rem;\n  margin: 8rem auto 0;\n  background-color: var(--element-background);\n\n  &__form {\n    .field:not(:last-child) {\n      margin-bottom: 2.4rem;\n    }\n  }\n\n  &__btn {\n    margin-top: 3.2rem;\n  }\n\n  &__error {\n    display: inline-block;\n    margin-top: 2.4rem;\n    color: var(--error-color);\n\n    &:empty {\n      display: none;\n    }\n  }\n}\n</style>\n"]}]}