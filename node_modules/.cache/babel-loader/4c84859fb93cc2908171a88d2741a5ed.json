n      <h1>Список пользователей</h1>\n      <button v-if=\"Token\" type=\"button\" class=\"btn _accent\" @click=\"openModal\">Создать</button>\n    </template>\n    <user-list :items=\"Users\"/>\n  </AppSection>\n</template>\n\n<script>\nimport AppSection from \"../components/AppSection\";\nimport UserList from \"../components/UserList\";\nimport {mapGetters, mapMutations} from \"vuex\";\n\nexport default {\n  name: \"Users\",\n  components: {AppSection, UserList},\n  computed: {\n    ...mapGetters({\n      Token: 'login/Token',\n      Users: 'user/Users'\n    })\n  },\n  mounted() {\n    console.log('Token: ', this.Token)\n    if (!!this.Token) {\n      this.$store.dispatch('user/getData', this.Token)\n    }\n  },\n  methods: {\n    ...mapMutations({ TOGGLE_MODAL_VISIBLE: 'modal/TOGGLE_MODAL_VISIBLE' }),\n    openModal() {\n      this.TOGGLE_MODAL_VISIBLE(true)\n    }\n  },\n}\n</script>\n<style lang=\"scss\">\n@import \"@/scss/app.scss\";\n\n.users {\n  display: flex;\n  flex-direction: column;\n  row-gap: 3.2rem;\n\n  &__filter {\n    display: block;\n    width: 100%;\n    border-radius: 2.4rem;\n    padding: 2.4rem;\n    background-color: var(--element-background);\n  }\n\n  &__btn-edit {\n    margin: auto;\n  }\n\n  &__undefined {\n    border-bottom: 1px solid var(--border-color);\n  }\n\n  &__not-able {\n    text-align: center;\n\n    a {\n      color: var(--accent-color);\n      text-decoration: underline;\n      opacity: 1;\n      transition: opacity $tr-default;\n\n      @include hover {\n        opacity: 0.8;\n      }\n    }\n  }\n}\n</style>"],"sourceRoot":""}]}