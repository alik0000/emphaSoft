CAAf","sourcesContent":["<template>\n  <AppSection tag=\"section\">\n    <template #head>\n      <h1>Список пользователей</h1>\n      <button v-if=\"!!token\" type=\"button\" class=\"btn _accent\" @click=\"openModal\">Создать</button>\n    </template>\n    <UserList :items=\"userItems\"/>\n  </AppSection>\n</template>\n\n<script>\nimport { onMounted, computed } from \"vue\";\nimport {useStore} from \"vuex\";\nimport AppSection from \"../components/AppSection\";\nimport UserList from \"../components/UserList\";\n\nexport default {\n  name: \"Users\",\n  components: {AppSection, UserList},\n  setup() {\n    const store = useStore()\n    const token = computed(() => store.getters[\"login/Token\"])\n\n    const changeLoading = (flag) => {\n      store.commit('modal/CHANGE_LOADING', flag)\n    }\n\n    onMounted(async () => {\n      if (!!token) {\n        changeLoading(true)\n        await store.dispatch('user/getData')\n        changeLoading(false)\n      }\n    })\n\n    return {\n      token,\n      userItems: computed(() => store.getters[\"user/UserItems\"]),\n      openModal: () => store.commit('modal/TOGGLE_MODAL_VISIBLE', true),\n    }\n  },\n}\n</script>\n<style lang=\"scss\">\n@import \"@/scss/app.scss\";\n\n.users {\n  display: flex;\n  flex-direction: column;\n  row-gap: 3.2rem;\n\n  &__filter {\n    display: block;\n    width: 100%;\n    border-radius: 2.4rem;\n    padding: 2.4rem;\n    background-color: var(--element-background);\n  }\n\n  &__btn-edit {\n    margin: auto;\n  }\n\n  &__undefined {\n    border-bottom: 1px solid var(--border-color);\n  }\n\n  &__not-able {\n    text-align: center;\n\n    a {\n      color: var(--accent-color);\n      text-decoration: underline;\n      opacity: 1;\n      transition: opacity $tr-default;\n\n      @include hover {\n        opacity: 0.8;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":""}]}